NODE_ENV=staging
VITE_APP_ENV=staging

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-staging-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-staging-auth-domain
VITE_FIREBASE_PROJECT_ID=your-staging-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-staging-storage-bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your-staging-messaging-sender-id
VITE_FIREBASE_APP_ID=your-staging-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-staging-measurement-id

# WebSocket Configuration
VITE_WEBSOCKET_URL=wss://staging-websocket.yourapp.com

# Stripe Configuration
VITE_STRIPE_PUBLIC_KEY=your-staging-stripe-public-key

# Application Configuration
VITE_COMPANY_NAME=Your Company Name
VITE_APP_URL=https://staging.yourapp.com

# Error Reporting Configuration
VITE_ENABLE_ERROR_REPORTING=true
VITE_SENTRY_DSN=https://your-staging-sentry-dsn

# Analytics Configuration
VITE_ENABLE_ANALYTICS=true
VITE_GA_MEASUREMENT_ID=your-staging-ga-id
VITE_POSTHOG_API_KEY=your-staging-posthog-key
VITE_POSTHOG_HOST=https://app.posthog.com

# Feature Flags
VITE_ENABLE_FEATURE_FLAGS=true
VITE_GROWTHBOOK_API_HOST=https://cdn.growthbook.io
VITE_GROWTHBOOK_CLIENT_KEY=your-staging-growthbook-key 