NODE_ENV=production
VITE_APP_ENV=production

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-production-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-production-auth-domain
VITE_FIREBASE_PROJECT_ID=your-production-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-production-storage-bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your-production-messaging-sender-id
VITE_FIREBASE_APP_ID=your-production-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-production-measurement-id

# WebSocket Configuration
VITE_WEBSOCKET_URL=wss://websocket.yourapp.com

# Stripe Configuration
VITE_STRIPE_PUBLIC_KEY=your-production-stripe-public-key

# Application Configuration
VITE_COMPANY_NAME=Your Company Name
VITE_APP_URL=https://yourapp.com

# Error Reporting Configuration
VITE_ENABLE_ERROR_REPORTING=true
VITE_SENTRY_DSN=https://your-production-sentry-dsn

# Analytics Configuration
VITE_ENABLE_ANALYTICS=true
VITE_GA_MEASUREMENT_ID=your-production-ga-id
VITE_POSTHOG_API_KEY=your-production-posthog-key
VITE_POSTHOG_HOST=https://app.posthog.com

# Performance Optimization
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Feature Flags
VITE_ENABLE_FEATURE_FLAGS=true
VITE_GROWTHBOOK_API_HOST=https://cdn.growthbook.io
VITE_GROWTHBOOK_CLIENT_KEY=your-production-growthbook-key 