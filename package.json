{
  "name": "web-engine-platform",
  "version": "1.0.0",
  "description": "A comprehensive web engine platform for web applications",
  "main": "index.js",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "dev:hot": "node scripts/dev-server.js",
    "build": "tsc -p tsconfig.build.json && vite build && npm run generate-sw",
    "preview": "vite preview",
    "test": "npm run ensure-test-dirs && jest",
    "test:watch": "npm run ensure-test-dirs && jest --watch",
    "test:coverage": "npm run ensure-test-dirs && jest --coverage",
    "test:theme:visual": "node scripts/theme-visual-regression.js",
    "test:theme:performance": "node scripts/theme-performance.js",
    "test:theme:cross-browser": "ts-node scripts/theme-cross-browser.ts",
    "test:theme:console": "npm run dev -- --mode=theme-check",
    "theme:console:check": "node scripts/console-error-check.js",
    "test:theme:browser:chrome": "cross-env BROWSER=chrome npm run test:theme:cross-browser",
    "test:theme:browser:firefox": "cross-env BROWSER=firefox npm run test:theme:cross-browser",
    "test:theme:browser:edge": "cross-env BROWSER=edge npm run test:theme:cross-browser",
    "test:theme:browser:safari": "cross-env BROWSER=safari npm run test:theme:cross-browser",
    "test:theme:browser:all": "npm run test:theme:browser:chrome && npm run test:theme:browser:firefox && npm run test:theme:browser:edge",
    "test:theme:all": "npm run test:theme:visual && npm run test:theme:performance && npm run test:theme:cross-browser && npm run theme:console:check",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:flows": "node scripts/cypress-user-flows.js run",
    "test:e2e:flows:create": "node scripts/cypress-user-flows.js create",
    "test:e2e:flows:list": "node scripts/cypress-user-flows.js list",
    "test:e2e:flows:generate-all": "node scripts/cypress-user-flows.js generate-all",
    "test:e2e:flows:report": "node scripts/cypress-user-flows.js report",
    "test:a11y": "jest --testMatch='**/__tests__/**/*.a11y.test.{ts,tsx}'",
    "test:a11y:ci": "jest --testMatch='**/__tests__/**/*.a11y.test.{ts,tsx}' --ci",
    "test:a11y:scan": "node scripts/a11y-test-runner.js",
    "test:visual-regression": "node scripts/visual-regression.js",
    "test:visual-regression:update": "node scripts/visual-regression.js --update-baseline",
    "test:chromatic": "npx chromatic --project-token=${CHROMATIC_PROJECT_TOKEN}",
    "test:chromatic:ci": "npx chromatic --project-token=${CHROMATIC_PROJECT_TOKEN} --exit-zero-on-changes --exit-once-uploaded",
    "test:chromatic:local": "npm run build-storybook && npx chromatic --storybook-build-dir=storybook-static --project-token=${CHROMATIC_PROJECT_TOKEN}",
    "test:visual-all": "npm run test:visual-regression && npm run test:chromatic",
    "test:performance": "node scripts/performance-testing.js",
    "test:performance:components": "node scripts/component-performance.js",
    "test:all": "npm run test && npm run test:coverage && npm run test:e2e && npm run test:a11y && npm run test:visual-regression && npm run test:performance && npm run test:chromatic",
    "test:dashboard": "node scripts/test-dashboard.js",
    "test:dashboard:fresh": "node scripts/kill-test-dashboard.js && node scripts/test-dashboard.js",
    "kill-dashboard": "node scripts/kill-test-dashboard.js",
    "test:coverage:dashboard": "npm test -- --coverage && npm run test:dashboard",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "type-check": "tsc --noEmit",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --ci --coverage --maxWorkers=2",
    "test:visual": "node scripts/visual-regression.js",
    "test:theme:visual:update": "node scripts/theme-visual-regression.js --update-baseline",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "validate": "npm run lint:check && npm run format:check && npm run type-check",
    "ci": "npm run clean && npm run validate && npm run test:ci && npm run build",
    "analyze": "vite build --mode analyze",
    "analyze:chunks": "vite build --mode analyze && node scripts/analyze-chunks.js",
    "analyze:treemap": "vite build --mode analyze && open dist/stats.html",
    "optimize:images": "vite build --mode optimize-images",
    "optimize:fonts": "vite build --mode optimize-fonts",
    "analyze:build": "node scripts/analyze-build.js",
    "track:performance": "node scripts/track-build-performance.js",
    "build:analyze": "npm run build ; npm run analyze:build ; npm run track:performance",
    "build:analytics": "node scripts/build-analytics/build-analytics.js",
    "analyze:unused": "node scripts/build-analytics/unused-code-detector.js",
    "analyze:dependencies": "node scripts/build-analytics/dependency-analyzer.js",
    "analyze:build-time": "node scripts/build-analytics/build-time-optimizer.js",
    "analyze:all": "npm run analyze:unused && npm run analyze:dependencies && npm run analyze:build-time",
    "benchmark:theme": "node scripts/theme-performance.js",
    "analyze:typescript": "node scripts/build-analytics/typescript-error-analyzer.js",
    "validate:themes": "node scripts/validate-themes.js",
    "validate:themes:ci": "node scripts/validate-themes.js --format=github",
    "validate:themes:test": "node scripts/validate-themes.js --test-mode --verbose",
    "validate:themes:report": "node scripts/ci-validate-themes.js",
    "theme:migrate": "ts-node scripts/migrate-theme.ts",
    "theme:migrate:all": "ts-node scripts/migrate-all-themes.ts",
    "theme:check": "ts-node scripts/check-theme.ts",
    "theme:validate:pre-commit": "ts-node scripts/pre-commit-theme-validator.ts",
    "theme:unify": "ts-node scripts/theme-unification.ts",
    "theme:unify:dry": "ts-node scripts/theme-unification.ts --dry-run",
    "theme:unify:verbose": "ts-node scripts/theme-unification.ts --verbose",
    "theme:error-check": "node scripts/console-error-check.js",
    "theme:explorer": "node scripts/theme-property-explorer.js",
    "theme:diff": "node scripts/theme-diff-utility.js",
    "codegen": "graphql-codegen --config codegen.ts",
    "migrations:build": "tsc --project ./tsconfig.json --module commonjs --outDir ./dist src/scripts/run-migrations.ts",
    "migrations:run": "node ./dist/scripts/run-migrations.js run",
    "migrations:status": "node ./dist/scripts/run-migrations.js status",
    "migrations:rollback-firebase": "node ./dist/scripts/run-migrations.js rollback-firebase",
    "migrations:rollback-app": "node ./dist/scripts/run-migrations.js rollback-app",
    "build:staging": "NODE_ENV=staging vite build --mode staging && npm run generate-sw",
    "build:production": "NODE_ENV=production vite build --mode production && npm run generate-sw",
    "deploy:staging": "firebase deploy --only hosting:staging",
    "deploy:production": "firebase deploy --only hosting:production",
    "lighthouse-audit": "lighthouse http://localhost:3000 --view --output json --output html --output-path ./lighthouse-report.html",
    "generate-sw": "workbox-cli generateSW workbox-config.js",
    "build:sw": "npm run build && npm run generate-sw",
    "optimize:bundle": "node scripts/optimize-bundle.js",
    "optimize:bundle:analyze": "npm run analyze:treemap && node scripts/optimize-bundle.js",
    "build:optimized": "npm run optimize:bundle && vite build --mode production",
    "build:module-nomodule": "vite build --mode production && node scripts/module-nomodule.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-docs": "node scripts/generate-component-docs.js",
    "ensure-test-dirs": "node scripts/ensure-test-dirs.js",
    "ci:parallel": "node scripts/ci-parallel-tests.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
