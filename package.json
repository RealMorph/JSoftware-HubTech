{
  "name": "web-engine-platform",
  "version": "1.0.0",
  "description": "A comprehensive web engine platform for web applications",
  "main": "index.js",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "dev:hot": "node scripts/dev-server.js",
    "build": "tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:theme:visual": "node scripts/theme-visual-regression.js",
    "test:theme:performance": "node scripts/theme-performance.js",
    "test:theme:cross-browser": "ts-node scripts/theme-cross-browser.ts",
    "test:theme:all": "npm run test:theme:visual && npm run test:theme:performance && npm run test:theme:cross-browser",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "type-check": "tsc --noEmit",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --ci --coverage --maxWorkers=2",
    "test:visual": "node scripts/visual-regression.js",
    "test:visual:update": "node scripts/visual-regression.js --update-baseline",
    "test:theme:visual:update": "node scripts/theme-visual-regression.js --update-baseline",
    "test:all": "npm run test ; npm run test:visual ; npm run test:theme:visual ; npm run benchmark:theme",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "validate": "npm run lint:check && npm run format:check && npm run type-check",
    "ci": "npm run clean && npm run validate && npm run test:ci && npm run build",
    "analyze": "vite build --mode analyze",
    "analyze:chunks": "vite build --mode analyze && node scripts/analyze-chunks.js",
    "analyze:treemap": "vite build --mode analyze && open dist/stats.html",
    "optimize:images": "vite build --mode optimize-images",
    "optimize:fonts": "vite build --mode optimize-fonts",
    "analyze:build": "node scripts/analyze-build.js",
    "track:performance": "node scripts/track-build-performance.js",
    "build:analyze": "npm run build ; npm run analyze:build ; npm run track:performance",
    "build:analytics": "node scripts/build-analytics/build-analytics.js",
    "analyze:unused": "node scripts/build-analytics/unused-code-detector.js",
    "analyze:dependencies": "node scripts/build-analytics/dependency-analyzer.js",
    "optimize:build": "node scripts/build-analytics/build-time-optimizer.js",
    "analyze:build-time": "node scripts/build-analytics/build-time-optimizer.js",
    "analyze:all": "npm run analyze:unused && npm run analyze:dependencies && npm run analyze:build-time",
    "benchmark:theme": "node scripts/theme-performance.js",
    "analyze:typescript": "node scripts/build-analytics/typescript-error-analyzer.js",
    "validate:themes": "node scripts/validate-themes.js",
    "validate:themes:ci": "node scripts/validate-themes.js --format=github",
    "validate:themes:report": "node scripts/ci-validate-themes.js",
    "theme:migrate": "ts-node scripts/migrate-theme.ts",
    "theme:migrate:all": "ts-node scripts/migrate-all-themes.ts",
    "theme:check": "ts-node scripts/check-theme.ts",
    "theme:validate:pre-commit": "ts-node scripts/pre-commit-theme-validator.ts",
    "theme:unify": "ts-node scripts/theme-unification.ts",
    "theme:unify:dry": "ts-node scripts/theme-unification.ts --dry-run",
    "theme:unify:verbose": "ts-node scripts/theme-unification.ts --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/s3-request-presigner": "^3.525.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@nestjs/platform-express": "^10.3.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/leaflet": "^1.9.17",
    "@types/react": "^19.1.0",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.8.4",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.5.0",
    "react-simple-maps": "^3.0.0",
    "recharts": "^2.15.2",
    "styled-components": "^6.1.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@nestjs/testing": "^10.3.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.17.30",
    "@types/react-dnd": "^3.0.2",
    "@types/react-dom": "^19.1.0",
    "@types/rollup-plugin-visualizer": "^4.2.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^29.7.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.2",
    "node-fetch": "^3.3.2",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.2.5",
    "pretty-ms": "^9.2.0",
    "puppeteer": "^21.11.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^5.1.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imagemin": "^0.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "glob": "^10.3.10"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
